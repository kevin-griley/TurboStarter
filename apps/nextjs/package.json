    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.37.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.3"
